{
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.server.extraEnv": {
        "SKIP_WASM_BUILD": "true"
    },
    "rust-analyzer.diagnostics.disabled": [
        "macro-error"
    ],
    "rust-analyzer.diagnostics.experimental.enable": false,
    "rust-analyzer.cargo.features": [
        "private-net",
        "wip",
        "stage",
        "runtime-benchmarks",
        "try-runtime",
    ]
}
